<?xml version="1.0" encoding="utf-8"?>
<resources>
    <drawable name="theme_default_wallpaper_overlay">@android:color/transparent</drawable>
    <drawable name="theme_default_wallpaper_overlay_when_dash_opened">@android:color/transparent</drawable>

    <!-- Launcher -->
    <integer name="theme_default_launcher_location">@integer/position_left</integer>
    <array name="theme_default_launcher_location_supported">
        <item>@integer/position_left</item>
        <item>@integer/position_right</item>
        <item>@integer/position_bottom</item>
    </array>
    <array name="theme_default_launcher_margin">
        <item>0dp</item>
        <item>0dp</item>
        <item>0dp</item>
        <item>0dp</item>
    </array>
    <bool name="theme_default_launcher_expand">true</bool>
    <bool name="theme_default_launcher_background_dynamic">true</bool>
    <array name="theme_default_launcher_background">
        <item>@android:color/transparent</item>
        <item>@android:color/transparent</item>
        <item>@android:color/transparent</item>
        <item>@android:color/transparent</item>
        <item>@android:color/transparent</item>
    </array>
    <integer name="theme_default_launcher_bfb_location">@integer/position_top</integer>
    <drawable name="theme_default_launcher_bfb_image">@drawable/launcher_bfb</drawable>
    <bool name="theme_default_launcher_bfb_hide_while_dragging">false</bool>
    <integer name="theme_default_launcher_preferences_location">@integer/position_bottom</integer>
    <drawable name="theme_default_launcher_preferences_image">@drawable/launcher_preferences</drawable>
    <integer name="theme_default_launcher_preferences_location_when_panel_hidden">@integer/position_bottom</integer>
    <drawable name="theme_default_launcher_trash_image">@drawable/launcher_trash</drawable>
    <bool name="theme_default_launcher_applauncher_backgroundcolour_dynamic">true</bool>
    <color name="theme_default_launcher_applauncher_backgroundcolour">@android:color/transparent</color>
    <drawable name="theme_default_launcher_applauncher_background">@drawable/widget_launcher_applauncher_background</drawable>
    <drawable name="theme_default_launcher_applauncher_gradient">@drawable/launcher_icon_bg</drawable>
    <drawable name="theme_default_launcher_applauncher_running">@drawable/launcher_app_running</drawable>
    <bool name="theme_default_launcher_applauncher_running_backgroundcolour_dynamic">true</bool>
    <color name="theme_default_launcher_applauncher_running_backgroundcolour">@android:color/transparent</color>

    <!-- Panel -->
    <integer name="theme_default_panel_location">@integer/position_top</integer>
    <array name="theme_default_panel_location_supported">
        <item>@integer/position_top</item>
        <item>@integer/position_none</item>
    </array>
    <dimen name="theme_default_panel_height">28dp</dimen>
    <drawable name="theme_default_panel_background">@drawable/panel_background</drawable>
    <bool name="theme_default_panel_background_dynamic_when_dash_opened">true</bool>
    <integer name="theme_default_panel_bfb_location">@integer/position_none</integer>
    <string name="theme_default_panel_bfb_text"></string>
    <color name="theme_default_panel_bfb_text_colour">@android:color/white</color>
    <integer name="theme_default_panel_close_location">@integer/position_left</integer>
    <drawable name="theme_default_panel_close_image">@drawable/dash_close</drawable>
    <integer name="theme_default_panel_preferences_location">@integer/position_right</integer>
    <drawable name="theme_default_panel_preferences_image">@drawable/panel_cog</drawable>
    <array name="theme_default_panel_swap_close_preferences_when_launcher_location">
        <item>@integer/position_right</item>
    </array>

    <!-- Dash -->
    <bool name="theme_default_dash_background_dynamic">true</bool>
    <drawable name="theme_default_dash_background">@android:color/transparent</drawable>
    <drawable name="theme_default_dash_background_gradient">@drawable/dash_background_gradient</drawable>
    <color name="theme_default_dash_applauncher_text_colour">@android:color/white</color>
    <color name="theme_default_dash_applauncher_text_shadow_colour">@android:color/black</color>
    <color name="theme_default_dash_customise_text_colour">@android:color/white</color>
    <color name="theme_default_dash_customise_text_shadow_colour">@android:color/black</color>
    <color name="theme_default_dash_customise_spinner_text_colour">@android:color/primary_text_dark</color>
    <drawable name="theme_default_dash_search_background">@drawable/dash_search_background</drawable>
    <color name="theme_default_dash_search_text_colour">@android:color/white</color>
    <bool name="theme_default_dash_ribbon_show">true</bool>
</resources>