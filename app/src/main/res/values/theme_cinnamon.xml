<?xml version="1.0" encoding="utf-8"?>
<resources>
    <drawable name="theme_cinnamon_wallpaper_overlay">@android:color/transparent</drawable>
    <drawable name="theme_cinnamon_wallpaper_overlay_when_dash_opened">@android:color/transparent</drawable>

    <!-- Launcher -->
    <integer name="theme_cinnamon_launcher_location">@integer/position_bottom</integer>
    <array name="theme_cinnamon_launcher_location_supported">
        <item>@integer/position_bottom</item>
        <item>@integer/position_top</item>
        <item>@integer/position_left</item>
        <item>@integer/position_right</item>
    </array>
    <array name="theme_cinnamon_launcher_margin">
        <item>0dp</item>
        <item>0dp</item>
        <item>0dp</item>
        <item>0dp</item>
    </array>
    <bool name="theme_cinnamon_launcher_expand">true</bool>
    <bool name="theme_cinnamon_launcher_background_dynamic">false</bool>
    <array name="theme_cinnamon_launcher_background">
        <item>@android:color/transparent</item>
        <item>@drawable/theme_cinnamon_res_launcher_background_top</item>
        <item>@drawable/theme_cinnamon_res_launcher_background_right</item>
        <item>@drawable/theme_cinnamon_res_launcher_background_bottom</item>
        <item>@drawable/theme_cinnamon_res_launcher_background_left</item>
    </array>
    <integer name="theme_cinnamon_launcher_bfb_location">@integer/position_left</integer>
    <drawable name="theme_cinnamon_launcher_bfb_image">@drawable/theme_cinnamon_res_launcher_bfb</drawable>
    <bool name="theme_cinnamon_launcher_bfb_hide_while_dragging">false</bool>
    <integer name="theme_cinnamon_launcher_preferences_location">@integer/position_right</integer>
    <drawable name="theme_cinnamon_launcher_preferences_image">@drawable/theme_cinnamon_res_launcher_preferences</drawable>
    <integer name="theme_cinnamon_launcher_preferences_location_when_panel_hidden">@integer/position_right</integer>
    <drawable name="theme_cinnamon_launcher_trash_image">@drawable/launcher_trash</drawable>
    <bool name="theme_cinnamon_launcher_applauncher_backgroundcolour_dynamic">false</bool>
    <color name="theme_cinnamon_launcher_applauncher_backgroundcolour">@android:color/transparent</color>
    <drawable name="theme_cinnamon_launcher_applauncher_background">@drawable/blank_gradient</drawable>
    <drawable name="theme_cinnamon_launcher_applauncher_gradient">@android:color/transparent</drawable>
    <drawable name="theme_cinnamon_launcher_applauncher_running">@drawable/theme_cinnamon_res_launcher_app_running</drawable>
    <bool name="theme_cinnamon_launcher_applauncher_running_backgroundcolour_dynamic">false</bool>
    <color name="theme_cinnamon_launcher_applauncher_running_backgroundcolour">@android:color/transparent</color>

    <!-- Panel -->
    <integer name="theme_cinnamon_panel_location">@integer/position_none</integer>
    <array name="theme_cinnamon_panel_location_supported">
        <item>@integer/position_none</item>
    </array>
    <dimen name="theme_cinnamon_panel_height">0dp</dimen>
    <drawable name="theme_cinnamon_panel_background">@android:color/transparent</drawable>
    <bool name="theme_cinnamon_panel_background_dynamic_when_dash_opened">false</bool>
    <integer name="theme_cinnamon_panel_bfb_location">@integer/position_none</integer>
    <string name="theme_cinnamon_panel_bfb_text">@android:string/no</string>
    <color name="theme_cinnamon_panel_bfb_text_colour">@android:color/white</color>
    <integer name="theme_cinnamon_panel_close_location">@integer/position_none</integer>
    <drawable name="theme_cinnamon_panel_close_image">@android:color/transparent</drawable>
    <integer name="theme_cinnamon_panel_preferences_location">@integer/position_none</integer>
    <drawable name="theme_cinnamon_panel_preferences_image">@android:color/transparent</drawable>
    <array name="theme_cinnamon_panel_swap_close_preferences_when_launcher_location">
    </array>

    <!-- Dash -->
    <bool name="theme_cinnamon_dash_background_dynamic">false</bool>
    <drawable name="theme_cinnamon_dash_background">@drawable/theme_cinnamon_res_dash_background</drawable>
    <drawable name="theme_cinnamon_dash_background_gradient">@android:color/transparent</drawable>
    <color name="theme_cinnamon_dash_applauncher_text_colour">@android:color/white</color>
    <color name="theme_cinnamon_dash_applauncher_text_shadow_colour">@android:color/transparent</color>
    <color name="theme_cinnamon_dash_customise_text_colour">@android:color/white</color>
    <color name="theme_cinnamon_dash_customise_text_shadow_colour">@android:color/transparent</color>
    <color name="theme_cinnamon_dash_customise_spinner_text_colour">@android:color/primary_text_dark</color>
    <drawable name="theme_cinnamon_dash_search_background">@drawable/theme_cinnamon_res_dash_search_background</drawable>
    <color name="theme_cinnamon_dash_search_text_colour">@android:color/white</color>
    <bool name="theme_cinnamon_dash_ribbon_show">false</bool>
</resources>