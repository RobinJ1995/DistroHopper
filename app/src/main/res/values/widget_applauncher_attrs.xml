<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AppLauncher">
        <!-- Let's just prefix everything with `applauncher_` as to avoid conflicts with v7-appcompat... -->
        <attr name="applauncher_label" format="string" />
        <attr name="applauncher_description" format="string" />
        <attr name="applauncher_icon" format="integer" />
        <attr name="applauncher_special" format="boolean" />
        <attr name="applauncher_background" format="integer" />
    </declare-styleable>
</resources>