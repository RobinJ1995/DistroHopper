<?xml version="1.0" encoding="utf-8"?>
<resources>
    <drawable name="theme_elementary_wallpaper_overlay">@android:color/transparent</drawable>
    <drawable name="theme_elementary_wallpaper_overlay_when_dash_opened">@android:color/transparent</drawable>

    <!-- Launcher -->
    <integer name="theme_elementary_launcher_location">@integer/position_bottom</integer>
    <array name="theme_elementary_launcher_margin">
        <item>0dp</item>
        <item>16dp</item>
        <item>0dp</item>
        <item>16dp</item>
    </array>
    <bool name="theme_elementary_launcher_expand">false</bool>
    <bool name="theme_elementary_launcher_background_dynamic">false</bool>
    <drawable name="theme_elementary_launcher_background">@drawable/theme_elementary_res_launcher_background</drawable>
    <integer name="theme_elementary_launcher_bfb_location">@integer/position_left</integer>
    <drawable name="theme_elementary_launcher_bfb_image">@drawable/theme_elementary_res_launcher_bfb</drawable>
    <integer name="theme_elementary_launcher_preferences_location">@integer/position_none</integer>
    <drawable name="theme_elementary_launcher_preferences_image">@drawable/theme_elementary_res_launcher_preferences</drawable>
    <integer name="theme_elementary_launcher_preferences_location_when_panel_hidden">@integer/position_right</integer>
    <bool name="theme_elementary_launcher_applauncher_backgroundcolour_dynamic">false</bool>
    <color name="theme_elementary_launcher_applauncher_backgroundcolour">@android:color/transparent</color>
    <drawable name="theme_elementary_launcher_applauncher_background">@drawable/blank_gradient</drawable>
    <drawable name="theme_elementary_launcher_applauncher_gradient">@android:color/transparent</drawable>
    <drawable name="theme_elementary_launcher_applauncher_running">@android:color/transparent</drawable>
    <bool name="theme_elementary_launcher_applauncher_running_backgroundcolour_dynamic">false</bool>
    <color name="theme_elementary_launcher_applauncher_running_backgroundcolour">@android:color/transparent</color>

    <!-- Panel -->
    <integer name="theme_elementary_panel_location">@integer/position_top</integer>
    <dimen name="theme_elementary_panel_height">32dp</dimen>
    <drawable name="theme_elementary_panel_background">@android:color/transparent</drawable>
    <bool name="theme_elementary_panel_background_dynamic_when_dash_opened">false</bool>
    <integer name="theme_elementary_panel_bfb_location">@integer/position_left</integer>
    <string name="theme_elementary_panel_bfb_text">@string/theme_elementary_res_panel_bfb_text</string>
    <color name="theme_elementary_panel_bfb_text_colour">@android:color/white</color>
    <integer name="theme_elementary_panel_close_location">@integer/position_none</integer>
    <drawable name="theme_elementary_panel_close_image">@android:color/transparent</drawable>
    <integer name="theme_elementary_panel_preferences_location">@integer/position_right</integer>
    <drawable name="theme_elementary_panel_preferences_image">@drawable/theme_elementary_res_panel_cog</drawable>

    <!-- Dash -->
    <bool name="theme_elementary_dash_background_dynamic">false</bool>
    <drawable name="theme_elementary_dash_background">@drawable/theme_elementary_res_dash_background</drawable>
    <drawable name="theme_elementary_dash_background_gradient">@android:color/transparent</drawable>
    <color name="theme_elementary_dash_applauncher_text_colour">#101010</color>
    <color name="theme_elementary_dash_applauncher_text_shadow_colour">@android:color/black</color>
    <drawable name="theme_elementary_dash_search_background">@drawable/theme_elementary_res_dash_search_background</drawable>
    <color name="theme_elementary_dash_search_text_colour">@android:color/black</color>
    <bool name="theme_elementary_dash_ribbon_show">false</bool>
</resources>